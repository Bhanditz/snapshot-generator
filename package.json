    "npm": ">=6"
  },
  "homepage": "https://github.com/w3c/snapshot-generator",
  "dependencies": {
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "mkdirp": "0.5.1",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "eslint-plugin-node": "8.0.0",
